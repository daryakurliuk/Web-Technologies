<mvc:View 
    controllerName="sap.ui.demo.walkthrough.controller.App" 
    xmlns="sap.m" 
    xmlns:l="sap.ui.layout" 
    xmlns:mvc="sap.ui.core.mvc"
    >
    <Panel class="sapUiResponsiveMargin" width="auto">
        <content>
            <List
                id="peopleList"
				headerText="{i18n>listHeader}"
				items="{
					path: 'People>/value',
					factory: '.listFactory'
				}">
					<ObjectListItem
						id="item"
                        type = "Active"
                        title = "{People>UserName}  {People>FirstName} {People>LastName} {People>Gender}" >
					</ObjectListItem>
                    </List>
        </content>
    </Panel>
</mvc:View>